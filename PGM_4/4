//     Print minimum number of notes required (1, 2, 5, 10, 20, 50, 100, 200, 500).
 Example: 1256 → 2 × 500, 1 × 200, 1 × 50, 1 × 5, 1 × 1.

 # include<stdio.h>
 int main()
 {
 int notes[]={500,200,100,50,20,10,5,2,1};
 int amount;

 printf(" enter a amount ");
 scanf("%d",&amount);

 printf("\n minimum number of notes required are \n");
 for(int i=0;i<9;i++)
 {
 int count=amount/notes[i];
 if(count>0)
 {
 printf("%d * rs %d\n",count,notes[i]);
 }
 amount=amount%notes[i];
 }
 }
